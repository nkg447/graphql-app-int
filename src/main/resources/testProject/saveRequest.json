{
  "schema": "type Post{\n    id: Int!\n    title: String!\n    body: String\n    user: User!\n    userId: Int!\n}\n\ntype Todo{\n    id: Int!\n    userId: User!\n    title: String!\n    completed: Boolean!\n}\n\ntype User{\n    id: Int!\n    name: String!\n    username: String!\n    email: String!\n    phone: String!\n    website: String!\n    company: Company!\n    address: Address!\n    posts: [Post!]!\n}\n\ntype Company{\n    catchPhrase: String!\n    name: String!\n    bs: String!\n}\n\ntype Address{\n    street: String!\n    suite: String!\n    city: String!\n    zipcode: String!\n}\n\ntype Geo{\n    lat: String!\n    lng: String!\n}\n\ntype Query{\n    getAllPosts: [Post!]!\n    getPostById(id: Int!): Post\n    getUserById(id: Int!): User\n    getPostsByUserId(id: Int!): [Post!]!\n}\n\ninput CreatePostRequest{\n    title: String!\n    body: String!\n    userId: Int!\n}\n\ninput UpdatePostRequest{\n    id: Int!\n    title: String!\n    body: String!\n    userId: Int!\n}\n\ntype CreatePostResponse{\n    id: Int!\n}\n\ntype Mutation{\n    createPost(input: CreatePostRequest!): CreatePostResponse\n    createPostWithTitle(title: String!, userId: Int!): CreatePostResponse\n    updatePost(input: UpdatePostRequest!): Post\n}\n\ntype Subscription{\n    noop: Int\n}",
  "restMappings": "[\n  {\n    \"name\": \"getAllPosts\",\n    \"endpoint\": \"https://jsonplaceholder.typicode.com/posts\",\n    \"method\": \"GET\"\n  },\n  {\n    \"name\": \"getPostById\",\n    \"endpoint\": \"https://jsonplaceholder.typicode.com/posts/${id}\",\n    \"method\": \"GET\"\n  },\n  {\n    \"name\": \"getUserById\",\n    \"endpoint\": \"https://jsonplaceholder.typicode.com/users/${id}\",\n    \"method\": \"GET\"\n  },\n  {\n    \"name\": \"getUserByUserId\",\n    \"endpoint\": \"https://jsonplaceholder.typicode.com/users/${userId}\",\n    \"method\": \"GET\"\n  },\n  {\n    \"name\": \"getPostsByUserId\",\n    \"endpoint\": \"https://jsonplaceholder.typicode.com/users/${id}/posts\",\n    \"method\": \"GET\"\n  },\n  {\n    \"name\": \"createPost\",\n    \"endpoint\": \"https://jsonplaceholder.typicode.com/posts\",\n    \"method\": \"POST\"\n  },\n  {\n    \"name\": \"updatePost\",\n    \"endpoint\": \"https://jsonplaceholder.typicode.com/posts/${id}\",\n    \"method\": \"PUT\"\n  },\n  {\n    \"name\": \"createPostWithTitle\",\n    \"endpoint\": \"https://jsonplaceholder.typicode.com/posts\",\n    \"method\": \"POST\",\n    \"requestBody\": \"{\\n    \\\"title\\\": \\\"${title}\\\",\\n    \\\"userId\\\": ${userId},\\n    \\\"body\\\": \\\"EOL\\\"\\n}\",\n    \"headers\": {\n      \"Accept\": \"application/json\",\n      \"h1\": \"q2\"\n    }\n  }\n]",
  "resolverMappings": "{\n  \"queryResolvers\": [\n    {\n      \"query\": \"getPostById\",\n      \"resolver\": {\n        \"restName\": \"getPostById\"\n      }\n    },\n    {\n      \"query\": \"getUserById\",\n      \"resolver\": {\n        \"restName\": \"getUserById\"\n      }\n    },\n    {\n      \"query\": \"getPostsByUserId\",\n      \"resolver\": {\n        \"restName\": \"getPostsByUserId\"\n      }\n    },\n    {\n      \"query\": \"getAllPosts\",\n      \"resolver\": {\n        \"restName\": \"getAllPosts\"\n      }\n    }\n  ],\n  \"mutationResolvers\": [\n    {\n      \"mutation\": \"createPost\",\n      \"resolver\": {\n        \"restName\": \"createPost\"\n      }\n    },\n    {\n      \"mutation\": \"updatePost\",\n      \"resolver\": {\n        \"restName\": \"updatePost\"\n      }\n    },\n    {\n      \"mutation\": \"createPostWithTitle\",\n      \"resolver\": {\n        \"restName\": \"createPostWithTitle\"\n      }\n    }\n  ],\n  \"typeResolvers\": [\n    {\n      \"type\": \"Post\",\n      \"keyResolvers\": [\n        {\n          \"key\": \"user\",\n          \"restName\": \"getUserByUserId\"\n        }\n      ]\n    },\n    {\n      \"type\": \"User\",\n      \"keyResolvers\": [\n        {\n          \"key\": \"posts\",\n          \"restName\": \"getPostsByUserId\"\n        }\n      ]\n    }\n  ]\n}"
}